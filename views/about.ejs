<%- include('./includes/head.ejs') %>

<body class="highlight-phone">
  <%- include('./includes/navigation.ejs') %>
    <div class="main-about">
        <div class="container">
            <div class="row">
                <div class="col-md-8">
                    <div class="intro">
                        <h2 class="over-img-text">Deconstruction</h2>
                        <p class="over-img-text">At the core of Decostruire photography is this ideal: deconstruct everything you thought you knew about fashion, about yourself, and about art.</p><a class="btn btn-primary" role="button" href="#">Discover yourself</a></div>
                </div>
                <div class="col-sm-4">
                    <div class="d-none d-md-block phone-mockup"></div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col">
            <div class="carousel slide" data-ride="carousel" id="carousel-1">
                <div class="carousel-inner">
                    <div class="carousel-item active"><img class="w-100 d-block" src="images/candles.jpg" alt="Slide Image"></div>
                    <div class="carousel-item"><img class="w-100 d-block" src="images/litup.jpg" alt="Slide Image"></div>
                    <div class="carousel-item"><img class="w-100 d-block" src="images/boa.jpg" alt="Slide Image"></div>
                </div>
                <div><a class="carousel-control-prev" href="#carousel-1" role="button" data-slide="prev"><span class="carousel-control-prev-icon"></span><span class="sr-only">Previous</span></a><a class="carousel-control-next" href="#carousel-1" role="button"
                        data-slide="next"><span class="carousel-control-next-icon"></span><span class="sr-only">Next</span></a></div>
                <ol class="carousel-indicators">
                    <li data-target="#carousel-1" data-slide-to="0" class="active"></li>
                    <li data-target="#carousel-1" data-slide-to="1"></li>
                    <li data-target="#carousel-1" data-slide-to="2"></li>
                </ol>
            </div>
        </div>
    </div>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.5.2/js/bootstrap.bundle.min.js"></script>
    <%- include('./includes/footer.ejs') %>
</body>

</html>
